{
  "name": "frontend-angular",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --port 4201",
    "start:local": "ng serve --port 4201 --disable-host-check",
    "start:dev": "ng serve --configuration=dev --port 4201 --disable-host-check",
    "build": "ng build --configuration production --base-href /admin/",
    "test": "ng test --no-watch",
    "test:start": "ng test --watch --browsers Chrome",
    "test:coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "i18n:extract": "ngx-translate-extract --input=src --output=src/assets/i18n/ru.json src/assets/i18n/en.json --format=namespaced-json --format-indentation=\"  \" --key-as-default-value --clean --sort"
  },
  "private": true,
